<import src="post-item/post-item.wxml"/>
<view>
  <swiper class='swiper' indicator-dots='true' autoplay='true' >
    <swiper-item>
      <image class='swiper-image' src='/image/ic.jpg'></image>
    </swiper-item>
    <swiper-item>
      <image  class='swiper-image'  src='/image/lt.jpg'></image>
    </swiper-item>
    <swiper-item>
      <image  class='swiper-image'  src='/image/tr.jpg'></image>
    </swiper-item>
  </swiper>
  <block  wx:for="{{post_key}}" wx:for-item="item">
      <template is = 'postItem' data = "{{item}}"/>
  </block>
</view>
